<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"		"http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.ly666.mapper.CategoryMapper">	<insert id="addCategory">		insert into category (cname, parentid)		values (#{cname},#{parentid});	</insert>	<select id="getCnameCategory" parameterType="string" resultType="Category">		select * from category where cname=#{cname}	</select>	<select id="categoryCount" resultType="integer">		select count(*) from category where cid not in (0)	</select>	<select id="getAllCategory" resultType="Category">		select * from category where cid not in (0)	</select>	<select id="getAllCategoryOrderByParentid" resultType="Category">		select * from category where cid not in (0) order by parentid	</select>	<select id="getCategory" parameterType="integer" resultType="Category">		select * from category where cid=#{cid}	</select>	<select id="getParentCategory" resultType="Category">		select * from category where parentid=#{parentid}	</select>	<update id="updateCategory">		update category set parentid=#{parentid},cname=#{cname} where cid=#{cid}	</update>	<delete id="deleteCategory">		delete from category where cid=#{cid}    </delete></mapper>